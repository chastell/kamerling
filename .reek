DuplicateMethodCall:
  exclude:
    - Kamerling::Handler#handle
    - Kamerling::Handler#process
    - Kamerling::Logging#log_dispatcher
    - Kamerling::Logging#log_server
    - Kamerling::Mapper#to_h
    - Kamerling::TaskDispatcher#dispatch_task

IrresponsibleModule:
  enabled: false

NestedIterators:
  max_allowed_nesting: 2
